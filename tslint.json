{
  "extends": "tslint-config-airbnb",
  "rules": {
    "max-line-length": {
      "severity": "warning",
      "options": 120
    },
    "trailing-comma": false,
    "variable-name": [true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case",
      "allow-trailing-underscore"
    ]
  }
}
